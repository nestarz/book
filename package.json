{
  "name": "eliasrhouzlane-book",
  "description": "Gatsby Portfolio",
  "version": "3.0.0",
  "author": "Elias Rhouzlane <elias.rhouzlane@gmail.com> (https://www.eliasrhouzlane.com)",
  "repository": {
    "url": "https://github.com/nestarz/book",
    "type": "git"
  },
  "private": true,
  "scripts": {
    "develop": "gatsby develop",
    "dev": "gatsby develop -o",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "lint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.jsx --fix --ignore-path .gitignore",
    "format": "prettier \"**/*.{md,mdx} \" --write",
    "lint:ci": "yarn lint --format junit -o results/eslint/result.xml",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome",
    "cy:run:ci": "cypress run --browser chrome --reporter junit --reporter-options 'mochaFile=results/cypress/result.xml'",
    "test:e2e:dev": "start-server-and-test dev http://localhost:8000 cy:open",
    "test:e2e:run": "start-server-and-test develop http://localhost:8000 cy:run",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run:ci"
  },
  "dependencies": {
    "@mdx-js/mdx": "0.16.6",
    "@mdx-js/tag": "0.16.6",
    "@rehooks/component-size": "^1.0.2",
    "babel-plugin-styled-components": "^1.10.0",
    "circular-std": "^1.0.0",
    "compromise": "^11.13.0",
    "face-api.js": "^0.18.0",
    "gatsby": "^2.0.91",
    "gatsby-cli": "^2.4.9",
    "gatsby-image": "^2.0.26",
    "gatsby-mdx": "^0.3.4",
    "gatsby-plugin-catch-links": "^2.0.9",
    "gatsby-plugin-google-analytics": "^2.0.9",
    "gatsby-plugin-lodash": "^3.0.3",
    "gatsby-plugin-manifest": "^2.0.13",
    "gatsby-plugin-netlify": "^2.0.6",
    "gatsby-plugin-offline": "^2.0.21",
    "gatsby-plugin-react-helmet": "^3.0.5",
    "gatsby-plugin-sharp": "^2.0.17",
    "gatsby-plugin-sitemap": "^2.0.4",
    "gatsby-plugin-styled-components": "^3.0.4",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^3.0.1",
    "gatsby-remark-responsive-iframe": "^2.0.8",
    "gatsby-remark-toc": "^1.2.0",
    "gatsby-source-filesystem": "^2.0.18",
    "gatsby-source-twitter": "^2.0.0",
    "gatsby-transformer-javascript-frontmatter": "^2.0.6",
    "gatsby-transformer-sharp": "^2.1.9",
    "gl-matrix": "^3.0.0",
    "libphonenumber-js": "^1.7.8",
    "lodash": "^4.17.11",
    "mobile-detect": "^1.4.3",
    "moment-timezone": "^0.5.23",
    "p5": "^0.7.3",
    "polished": "^2.3.1",
    "prop-types": "^15.6.2",
    "qrcode.react": "^0.9.2",
    "react": "^16.8.0-alpha.0",
    "react-dom": "^16.8.0-alpha.0",
    "react-graph-vis": "^1.0.2",
    "react-helmet": "^6.0.0-beta",
    "react-icons": "^3.2.2",
    "react-spring": "^7.2.4",
    "react-timeago": "^4.3.0",
    "react-use": "^5.2.1",
    "react-webcam": "^1.1.0",
    "reactn": "^0.1.9",
    "recurrent-js": "^1.7.4",
    "styled-components": "^4.1.3",
    "styled-theming": "^2.2.0",
